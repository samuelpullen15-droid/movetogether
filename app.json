{
  "expo": {
    "name": "MoveTogether",
    "slug": "movetogether",
    "scheme": "movetogether",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "studio.designspark.movetogether"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "studio.designspark.movetogether"
    },
    "plugins": ["expo-router", "expo-apple-authentication"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
