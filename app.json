{
  "expo": {
    "name": "MoveTogether",
    "slug": "movetogether",
    "scheme": "movetogether",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "studio.designspark.movetogether",
      "icon": "./assets/icon-ios-1024x1024.png",
      "splash": {
        "image": "./assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "entitlements": {
        "com.apple.developer.healthkit": true
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "processing"
        ],
        "BGTaskSchedulerPermittedIdentifiers": [
          "com.expo.modules.backgroundtask.processing",
          "background-health-sync"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "NSCalendarsUsageDescription": "MoveTogether uses your calendar to schedule workout reminders and competition events.",
        "NSContactsUsageDescription": "MoveTogether can access contacts to invite friends to competitions.",
        "NSCameraUsageDescription": "MoveTogether uses the camera to take profile photos.",
        "NSPhotoLibraryUsageDescription": "MoveTogether needs access to your photos to upload profile pictures.",
        "NSLocationWhenInUseUsageDescription": "MoveTogether uses your location to track outdoor workouts."
      },
      "appleTeamId": "VSMAARVPY8"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "studio.designspark.movetogether",
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.ACTIVITY_RECOGNITION"
      ]
    },
    "plugins": [
      "./plugins/activity-ring-view",
      "./plugins/liquid-glass-button",
      "./plugins/liquid-glass-icon-button",
      "./plugins/liquid-glass-morphing-menu",
      "./plugins/withActivitySummaryModule",
      [
        "@kingstinct/react-native-healthkit",
        {
          "NSHealthShareUsageDescription": "MoveTogether needs access to your health data to track your activity rings, workouts, weight, and fitness progress for competitions.",
          "NSHealthUpdateUsageDescription": "MoveTogether needs permission to save your workout data and fitness metrics to Apple Health.",
          "useBackgroundRefresh": true
        }
      ],
      "expo-router",
      "expo-asset",
      "expo-video",
      "react-native-edge-to-edge",
      "expo-apple-authentication",
      [
        "expo-calendar",
        {
          "calendarPermission": "MoveTogether uses your calendar to schedule workout reminders and competition events.",
          "remindersPermission": "MoveTogether uses your reminders to schedule workout reminders and competition events."
        }
      ],
      "expo-web-browser",
      [
        "expo-background-fetch",
        {
          "ios": {
            "backgroundModes": [
              "fetch"
            ]
          }
        }
      ],
      "@react-native-community/datetimepicker",
      [
        "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
      ],
      "expo-font",
      "expo-mail-composer",
      "expo-secure-store",
      "expo-sqlite",
      [
        "@intercom/intercom-react-native",
        {
          "appId": "grfssyi8",
          "androidApiKey": "android_sdk-357badbab4e423f93ec1356083b81de9172e779f",
          "iosApiKey": "ios_sdk-ccd340dec50d2029f59643e46b8075b710682bd7"
        }
      ],
      "@sentry/react-native"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "jsEngine": "hermes",
    "developer": {
      "tool": "expo-cli"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5f01f240-d39e-4c56-a22e-b6d18b0a20d3"
      }
    }
  }
}
